{
	"manifest_version":2,
	"name":"ElementSniper",
	"version":"0.0.1",
	"description":"Sniping Elements for element details",
	"content_scripts":[{
		"js":["content.js"],
		"matches":["<all_urls>"]
	}],
	"background":{"scripts":["background.js"]},
	"browser_action":{"default_title":"ElementSniper"},
	"permissions":["activeTab","tabs","storage"]
}